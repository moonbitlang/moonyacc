{
  "is-main": true,
  "import": [
    "moonbitlang/x/fs",
    "moonbitlang/x/sys",
    "Yoorkin/ArgParser",
    "moonbitlang/yacc/lib/driver",
    "moonbitlang/yacc/lib/codegen",
    "moonbitlang/yacc/lib/codegen/gen_mbt",
    "moonbitlang/yacc/lib/driver/util",
